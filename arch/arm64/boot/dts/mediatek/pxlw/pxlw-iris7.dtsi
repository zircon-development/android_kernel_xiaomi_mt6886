
#include "pxlw-iris7-cfg-xm-m12-42-02-0a-dsc-cmd.dtsi"
#include "pxlw-iris7-cfg-xm-m12-36-02-0b-dsc-cmd.dtsi"

&i2c1 {
	clock-frequency = <400000>;
	#address-cells = <1>;
	#size-cells = <0>;
	pixelworks@26 {  //i2c: 22, i3c: 26
		status = "okay";
		compatible = "pixelworks,iris";
		reg = <0x26>;
	};

	pixelworks@22 {  //i2c: 22, i3c: 26
		status = "okay";
		compatible = "pixelworks,iris-i2c";
		reg = <0x22>;
	};
};
