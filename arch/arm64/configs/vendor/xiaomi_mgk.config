# Fingerprint
CONFIG_MTK_FINGERPRINT_SUPPORT=m
#CONFIG_GOODIX_CAP_FINGERPRINT=m
#CONFIG_FPC_FINGERPRINT=m
CONFIG_GOODIX_FINGERPRINT=m

# IIO
CONFIG_IIO_TRIGGERED_BUFFER=y

# Proximity
# CONFIG_MTK_ULTRASND_PROXIMITY is not set

# Supply
# CONFIG_MTK_PUMP_EXPRESS_50_PLUS_SUPPORT is not set
# CONFIG_MTK_HVBP_SUPPORT is not set
CONFIG_XMEXT_TI_GAUGE=m
CONFIG_XMEXT_SC8551A_CHG_PUMP=m
CONFIG_XM_PD_MANAGER=m
# CONFIG_CHARGER_RT9490 is not set
# CONFIG_SND_SOC_MT6338_ACCDET is not set
# CONFIG_SND_SOC_MT6368_ACCDET is not set

# TEE
CONFIG_MITEE_SUPPORT=m

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# UFS
CONFIG_SCSI_UFS_XIAOMI=y
CONFIG_SCSI_UFS_XIAOMI_CLD=y
CONFIG_SCSI_UFS_XIAOMI_FFU=y
CONFIG_SCSI_UFS_XIAOMI_EBUFF=y

# USB
CONFIG_MTK_USB_OFFLOAD=n
