# Camera
CONFIG_BUILD_XIAOMI_CAMERA_IMAGE_NAMES="zircon"
CONFIG_CUSTOM_KERNEL_IMGSENSOR="zircons5khp3sunny_mipi_raw zirconov16a1qsunny_mipi_raw zirconimx355sunny_mipi_raw zirconov02b10sunny_mipi_raw"
CONFIG_MTK_TINYSYS_SCP_DEBUG_SUPPORT=y
CONFIG_MTK_VIDEO_ZIRCON_LM3644=m
CONFIG_REGULATOR_WL2866D1=m
CONFIG_REGULATOR_WL2866D2=m

# Codec
CONFIG_SND_SMARTPA_AW882XX=m
CONFIG_SND_SOC_MT6368_ACCDET=m
CONFIG_USB_SWITCH_ET7480=m

# DRM
CONFIG_DRM_PANEL_N16U_42_02_0A_DSC_VDO=m
CONFIG_MI_DISP_DSC2712=y
CONFIG_MI_DISP_ESD_CHECK=y
CONFIG_MI_DISP_FOD_SYNC=y
CONFIG_MI_DISP_LHBM=y
CONFIG_MTK_SEC_VIDEO_PATH_SUPPORT=y

# Fingerprint
CONFIG_GOODIX_FINGERPRINT=m
CONFIG_MTK_FINGERPRINT_SUPPORT=m

# HWID
CONFIG_MI_HARDWARE_ID=m

# Haptic
CONFIG_INPUT_AW_HAPTIC=m

# NFC
CONFIG_MI_CAMLOG=m
CONFIG_MTK_CLKBUF_NFC=y
CONFIG_NFC=y
CONFIG_NFC_NXP_I2C=m
CONFIG_NXP_P73_DEVICES=m
CONFIG_TMS_ESE_DEVICE=m
CONFIG_TMS_GUIDE_DEVICE=m
CONFIG_TMS_NFC_DEVICE=m

# Proximity
CONFIG_MTK_ULTRASND_PROXIMITY=m

# Supply
CONFIG_BUILD_PRODUCT_NAME="zircon"
CONFIG_XMEXT_CHG_PUMP=m

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# VOW
CONFIG_MTK_VOW_AMAZON_SUPPORT=n
CONFIG_MTK_VOW_GVA_SUPPORT=y
CONFIG_MTK_VOW_MAX_PDK_NUMBER=0
